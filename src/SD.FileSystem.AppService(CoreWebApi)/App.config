<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--SD.Framework配置节点-->
    <section name="sd.framework" type="SD.Infrastructure.FrameworkSection, SD.Infrastructure" />
    <!--SD.IOC配置节点-->
    <section name="sd.ioc" type="SD.IOC.Core.DependencyInjectionSection, SD.IOC.Core" />
    <!--SD.CacheManager配置节点-->
    <section name="sd.cacheManager" type="SD.CacheManager.CacheManagerSection, SD.CacheManager" />
    <!--SD.Toolkits.Redis配置节点-->
    <section name="sd.toolkits.redis" type="SD.Toolkits.RedisSection, SD.Toolkits.Redis" />
    <!--SD.Toolkits.AspNet配置节点-->
    <section name="sd.toolkits.aspNet" type="SD.Toolkits.AspNet.AspNetSection, SD.Toolkits.AspNet" />
    <!--SD.Membership配置节点-->
    <section name="sd.membership" type="SD.IdentitySystem.MembershipSection, SD.IdentitySystem.Membership" />
  </configSections>

  <!--连接字符串-->
  <connectionStrings>
    <!--默认连接字符串-->
    <add name="DefaultConnection" connectionString="Data Source=.;Initial Catalog=SD.FileSystem;User Id=sa;Password=broadmall123!;MultipleActiveResultSets=true;" />
  </connectionStrings>

  <!--SD.Framework配置-->
  <sd.framework>
    <!--数据库读连接名称-->
    <database.read.connectionName>DefaultConnection</database.read.connectionName>
    <!--数据库写连接名称-->
    <database.write.connectionName>DefaultConnection</database.write.connectionName>
    <!--实体所在程序集-->
    <entity.assembly>SD.FileSystem.Domain</entity.assembly>
    <!--实体配置所在程序集-->
    <entity.config.assembly>SD.FileSystem.Repository</entity.config.assembly>
    <!--分区数量-->
    <partitions.count>1</partitions.count>
  </sd.framework>

  <!--SD.IOC配置-->
  <sd.ioc>
    <!--As接口程序集列表-->
    <asInterfaceAssemblies>
      <assembly name="SD.FileSystem.Repository" />
    </asInterfaceAssemblies>
    <!--As自身程序集列表-->
    <asSelfAssemblies>
      <assembly name="SD.FileSystem.AppService" />
    </asSelfAssemblies>
    <!--As接口类型列表-->
    <asInterfaceTypes>
      <type name="SD.Infrastructure.Repository.EntityFrameworkCore.DbCleaner" assembly="SD.Infrastructure.Repository.EntityFrameworkCore" />
    </asInterfaceTypes>
    <!--As基类类型列表-->
    <asBaseTypes>
      <type name="SD.FileSystem.Repository.Base.DbSession" assembly="SD.FileSystem.Repository" />
    </asBaseTypes>
  </sd.ioc>

  <!--SD.CacheManager配置-->
  <sd.cacheManager>
    <!--缓存提供者，Redis-->
    <cacheProvider type="SD.CacheManager.RedisProvider" assembly="SD.CacheManager.Redis" />
  </sd.cacheManager>

  <!--SD.Toolkits.Redis配置-->
  <sd.toolkits.redis password="">
    <!--服务器地址列表-->
    <endpoints>
      <endpoint name="" host="localhost" port="6379" />
    </endpoints>
  </sd.toolkits.redis>

  <!--SD.Toolkits.AspNet配置-->
  <sd.toolkits.aspNet authorized="false">
    <!--Host地址列表-->
    <hosts>
      <host port="4987" protocol="http" />
    </hosts>
    <!--静态文件根目录-->
    <staticFiles>Content</staticFiles>
    <!--文件服务器根目录-->
    <fileServer>Files</fileServer>
  </sd.toolkits.aspNet>

  <!--SD.Membership配置-->
  <sd.membership>
    <!--Membership提供者-->
    <provider type="SD.IdentitySystem.Membership.AspNetCore.MembershipProvider" assembly="SD.IdentitySystem.Membership.AspNetCore" />
  </sd.membership>
</configuration>
